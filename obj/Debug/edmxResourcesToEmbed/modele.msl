    </EntitySetMapping>
    <EntitySetMapping Name="vClientNumero">
      <EntityTypeMapping TypeName="HelloEntityFramework.vClientNumero">
        <MappingFragment StoreEntitySet="vClientNumero">
          <ScalarProperty Name="Prenom" ColumnName="Prenom" />
